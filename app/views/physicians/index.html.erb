<% content_for :title, "Physicians" %>

<table class="index_table">
  <tr>
    <th>ID</th>
    <th>Last Name</th>
    <th>First Name</th>
    <th>Office Number</th>
    <th>Specialty</th>
    <th>Address</th>
    <th>Phone #</th>
    <th>Email</th>
